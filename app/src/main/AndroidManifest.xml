<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.worker8.learningdagger">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BasicActivity" />
        <activity android:name=".CatActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ScopeActivity" />
        <activity android:name=".multibinding.MultiBindingActivity" />
        <activity android:name=".multibinding.RabbitActivity" />
        <activity android:name=".multibinding.JackRabbitActivity" />
        <activity android:name=".subcomponent.SubComponentActivity" />
        <activity android:name=".constructorInjection.ConstructorInjectionActivity" />

    </application>

</manifest>